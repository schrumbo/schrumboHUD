{
	"schemaVersion": 1,
	"id": "schrumbohud",
	"version": "${version}",
	"name": "schrumboHUD",
	"description": "Customizable Inventory HUD!",
	"authors": [
		"schrumbo"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/schrumbohud",
